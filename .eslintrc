{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-explicit-any": 0,
    "no-empty-function": "warn",
    "@typescript-eslint/no-empty-function": "warn",
    "no-var": "warn",
    "prefer-const": "warn"
  }
}
